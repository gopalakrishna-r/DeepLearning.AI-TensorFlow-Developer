  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {
    "execution": {
     "iopub.execute_input": "2021-06-16T15:23:11.403321Z",
     "iopub.status.busy": "2021-06-16T15:23:11.402650Z",
