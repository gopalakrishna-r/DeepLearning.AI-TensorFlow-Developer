   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {
    "execution": {
     "iopub.execute_input": "2021-06-16T15:23:11.440145Z",
     "iopub.status.busy": "2021-06-16T15:23:11.439403Z",
     "iopub.status.idle": "2021-06-16T15:23:11.441661Z",
